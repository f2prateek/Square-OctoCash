{
    "manifest_version": 2,
    "name": "Square OctoCash",
    "description": "Sponsor a beer to your oss devs.",
    "homepage_url": "https://github.com/f2prateek/Square-OctoCash",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "*://github.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://github.com/*"
            ],
            "js": [
                "inject-square-cash.js"
            ]
        }
    ]
}